{
  "name": "@itsfaraaz/ytsr",
  "version": "0.0.2-development",
  "description": "Improved implementation of node-ystr",
  "homepage": "https://github.com/itsfaraaz/node-ytsr#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/itsfaraaz/node-ytsr.git"
  },
  "license": "MIT",
  "author": "Faraaz Khan (originally by Tobias Kutscha)",
  "main": "./lib/main.js",
  "types": "./typings/index.d.ts",
  "files": [
    "lib",
    "typings"
  ],
  "scripts": {
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "lint:typings": "tslint typings/index.d.ts",
    "lint:typings:fix": "tslint --fix typings/index.d.ts",
    "mocha": "mocha -- -t 16000 test/*-test.js",
    "mocha:coverage": "mocha -- -t 16000 test/*-test.js --exclude test/e2e-test.js",
    "nyc": "nyc --reporter=lcov npm run-script mocha",
    "nyc:coverage": "nyc --reporter=lcov npm run-script mocha:coverage",
    "test": "npm run-script nyc",
    "test:coverage": "npm run-script nyc:coverage && codecov"
  },
  "dependencies": {
    "miniget": "^4.2.2"
  },
  "devDependencies": {
    "assert-diff": "^3.0.2",
    "codecov": "^3.8.1",
    "dtslint": "^4.0.8",
    "eslint": "^7.23.0",
    "mocha": "^10.2.0",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=8"
  }
